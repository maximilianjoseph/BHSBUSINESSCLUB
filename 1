Java max.js

var Preferred_Equity
var Common_Equity
var  Liabilities
var Price_per_share
var Stock_Price
var Total_Assets
var Intangible_Assets
var Market_Cap
var Earnings_per_share
var Earnings_growth_projected_5_years
var Reward
var Risk
function calculate() {
   console.log("Berkeley High Business club is not responsible for any loss in profit, this cannot predict the market.")
   console.log("Welcome to Berkeley High Business club Capital Calculator  by Maximilian Joseph")
   Preferred_Equity = parseInt(document.getElementById("enter preferred equity: ").value)
   Common_Equity = parseInt(document.getElementById("Common Equity: ").value)
   Liabilities = parseInt(document.getElementById("Liabilities: ").value)


   Total_Capital = Preferred_Equity + Common_Equity + Liabilities;
   console.log("Your total Capital is");
   console.log(Total_Capital);
   document.getElementById("Your total Capital is").innerHTML =Total_Capital

   console.log("Welcome to Berkeley High Business club P/E Ratio Calculator by Maximilian Joseph")
   Price_per_share = parseInt(document.getElementById("enter price per share:").value)
   Earnings_per_share = parseInt(document.getElementById("enter earnings per share:").value)


   PE_Ratio = Price_per_share / Earnings_per_share
   console.log("Your P/E Ratio is")
   console.log(PE_Ratio)
   document.getElementById("Your P/E Ratio is").innerHTML =PE_Ratio


   console.log("Welcome to Berkeley High Business club P/B ratio calulator by Maximilian Joseph")
   Stock_Price = parseInt(document.getElementById("Enter Stock Price: ").value)
   Total_Assets = parseInt(document.getElementById("Enter Total Assets: ").value)
   Intangible_Assets = parseInt(document.getElementById("Enter Inangible Assets: ").value)
   Liabilities = parseInt(document.getElementById("Liabilities: ").value)


   PB_Ratio = Stock_Price / (Total_Assets - Intangible_Assets + Liabilities);
   console.log("Your P/B Ratio is");
   console.log(PB_Ratio);
   document.getElementById("Your P/B Ratio is").innerHTML =PB_Ratio

   console.log("Welcome to Berkeley Business club Market Value calulator by Maximilian Joseph")
   Market_Cap = parseInt(document.getElementById("Enter Market Cap: ").value)
   Stock_Price = parseInt(document.getElementById("Enter Stock Price: ").value)

    Market_Value = Market_Cap * Stock_Price;
   console.log("Your Market Value is");
   console.log(Market_Value);
   document.getElementById("Your Market Value is").innerHTML = Market_Value

   console.log("Welcome to Berkeley High Business club Benjamin Graham Intrinsic value calulator by Maximilian Joseph")
   Earnings_per_share = parseInt(document.getElementById("Enter Earnings Per Share for the last 12 months: ").value)
   Earnings_growth_projected_5_years = parseInt(document.getElementById("Enter projected growth earnings for next five years: ").value)


   V = ((Earnings_per_share * (8.5 + 2 * Earnings_growth_projected_5_years) * 4.4) / 3.5);
   console.log('Intrinsic value is');
   console.log(V);
   document.getElementById("Intrinsic value is").innerHTML = V

   console.log("Welcome to Berkeley Business club Risk Reward calulator by Maximilian Joseph")
   Reward = parseInt(document.getElementById("Enter your Reward: ").value)
   Risk = parseInt(document.getElementById("Enter your Risk: ").value)

    Risk_Reward_Ratio = Reward / Risk;
   console.log("Your Risk Reward Ratio is");
   console.log(Risk_Reward_Ratio);
   document.getElementById("Your Risk Reward Ratio is").innerHTML =Risk_Reward_Ratio

}
